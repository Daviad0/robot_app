<ItemDrawer>:
    theme_text_color: "Custom"
    on_release: root.clickItem()

    IconLeftWidget:
        id: icon
        icon: root.icon
        on_release: root.clickItem()
        theme_text_color: "Custom"
        text_color: root.text_color


<ContentNavigationDrawer>:
    orientation: "vertical"
    padding: ("20dp", "8dp", "8dp", "8dp")
    spacing: "8dp"
    StackLayout:
        size_hint_y: None
        padding: ("20dp", "8dp", "8dp", "0dp")
        height: self.minimum_height
        AnchorLayout:
            anchor_x: "left"
            size_hint_y: None
            height: avatar.height

            Image:
                id: avatar
                size_hint: None, None
                size: "40dp", "40dp"
                source: "assets/sparkclub.png"
        StackLayout:
            size_hint_y: None
            padding: ("0dp", "8dp", "0dp", "8dp")
            height: self.minimum_height
            MDLabel:
                text: "test_1"
                id: username
                font_style: "Button"
                adaptive_height: True
                theme_text_color: "Custom"
                text_color: root.getColor("primary")

            MDLabel:
                text: "test1@gmail.com"
                id: email
                font_style: "Caption"
                adaptive_height: True
                theme_text_color: "Custom"
                text_color: root.getColor("secondary")

    StackLayout:

        DrawerList:
            id: md_list
    EmptySpace:
        size_hint_y: None
        height: '20dp'
    ScrollView:

        DrawerList:
            id: md_list2
        


