                    id: send_message
                    padding: '12dp'
                    font_size: '12sp'
                    pos_hint: {'center_x': .5, 'center_y': .5}
                    md_bg_color: root.getColor('primary')
                    text: 'Send'
                    on_release: root.sendMessage()
            
        

