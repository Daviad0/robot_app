<Cable>:
    canvas:
        Color:
            rgba: root.getColor('white')
        Rectangle:
            pos: self.pos
            size: self.size
    name: "cable"
    padding: '40dp'
    StackLayout:
        padding: ("0dp", "60dp", "0dp", "0dp")
        FloatLayout:
            size_hint_y: .1
            padding: '20dp'
            MDFillRoundFlatButton:
                id: cable_channel
                padding: '12dp'
                font_size: '12sp'
                pos_hint: {'center_x': .5, 'center_y': .5}
                md_bg_color: root.getColor('primary')
                text: 'Announcements - Lightning Robotics'
                on_release: root.showDropdown()
        ScrollView:
            do_scroll_x: False
            do_scroll_y: True
            size_hint_y: .8
            pos_hint: {'center_x': .5, 'center_y': .5}
            StackLayout:
                size_hint_y: None
                spacing: '12dp'
                id: messages
                height: self.minimum_height
                
                
        FloatLayout:
            size_hint_y: .1
            padding: '20dp'
            GridLayout:
                rows: 1
                size_hint_x: .8
                pos_hint: {'center_x': .5, 'center_y': .5}
                spacing:"10dp"
                FloatLayout:
                    size_hint_y: None
                    MDTextField:
                        pos_hint: {'center_x': .5, 'center_y': .5}
                        line_color_focus: root.getColor('primary')
                        line_color_normal: root.getColor('primary')
                        color_mode: 'custom'
                        active_line:True
                        id: create_message
                        hint_text:"Your Message"
                        font_size:"12sp"
                        font_name: 'Roboto'
                        border:(2,2,2,2)
                        padding:"20dp"
