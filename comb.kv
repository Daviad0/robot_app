<MessageBox>:
    canvas:
        Color:
            rgba: (0,0,0, .4)
        Rectangle:
            size: self.size
            pos: self.pos
    MDCard:
        radius: "10dp"
        elevation: '4dp'
        pos_hint: {'center_x': .5, 'center_y': .5}
        size_hint_x: .6
        size_hint_y: None
        md_bg_color: root.getColor('white')
        height: message_box_layout.height
        StackLayout:
            size_hint_y: None
            height: self.minimum_height
            id: message_box_layout
            padding: "14dp"
            spacing: '0dp'
            FloatLayout:
                size_hint_y: None
                height: spin_container.height
                pos_hint: {'center_x': 0.5, 'center_y': 0.5}
                StackLayout:
                    
                    size_hint_y: None
                    size_hint_x: None
                    pos_hint: {'center_x': 0.5, 'center_y': 0.5}
                    padding: "12dp"
                    height: spinnnnn.height + 36
                    id: spin_container
                    MDSpinner:
                        id: spinnnnn
                        size_hint: None, None
                        size: "20dp", "20dp"
                        pos_hint: {'center_x': .5, 'center_y': .5}
                        active: True
                        palette:
                            root.getColor("primary"), root.getColor("secondary"), root.getColor("light")
                    
            Label:
                text: "Please Wait..."
                id: message
                font_name: 'Roboto'
                font_size: '16sp'
                pos_hint: {'center_x': 0.5}
                size_hint_y: None
                color: root.getColor('secondary')
                bold: True
            Label:
                text: "Contacting Server..."
                id: submessage
                font_name: 'Roboto'
                font_size: '12sp'
                pos_hint: {'center_x': 0.5}
                size_hint_y: None
                color: root.getColor('secondary')
            
<ActionBox>:
    canvas:
        Color:
            rgba: (0,0,0, .4)
        Rectangle:
            size: self.size
            pos: self.pos
    MDCard:
        radius: "10dp"
        elevation: '4dp'
        pos_hint: {'center_x': .5, 'center_y': .5}
        size_hint_x: .8
        size_hint_y: None
        md_bg_color: root.getColor('white')
        height: action_box_layout.height
        id: card
        StackLayout:
            id: action_box_layout
            size_hint_y: None
            height: self.minimum_height
            padding: '20dp'
            Label:
                text: "Do This Action?"
                id: title
                font_name: 'Roboto'
                font_size: '16sp'
                pos_hint: {'center_x': 0.5}
                size_hint_y: None
                color: root.getColor('primary')
                bold: True
            Label:
                text: "Here are some details about the action you are about to do!"
                id: contents
                font_name: 'Roboto'
                font_size: '12sp'
                pos_hint: {'center_x': 0.5}
                size_hint_y: None
                color: root.getColor('secondary')
                text_size: self.width, None
                height: self.texture_size[1]
                halign: 'center'
                markup: True
            StackLayout:
                size_hint_y: None
                height: self.minimum_height
                padding: ('20dp', '20dp', '20dp', '0dp')
                spacing: '4dp'
                FloatLayout:
                    size_hint_y: None
                    pos_hint: {'center_x': 0.5, 'center_y': 0.5}
                    GridLayout:
                        rows: 1
                        id: buttons
                        size_hint_y: None
                        size_hint_x: None
                        width: self.minimum_width
                        pos_hint: {'center_x': 0.5, 'center_y': 0.5}
                        spacing: "5dp"
                        padding: "5dp"
                
                FloatLayout:
                    size_hint_y: None
                    pos_hint: {'center_x': 0.5, 'center_y': 0.5}
                    GridLayout:
                        rows: 1
                        id: defaultbuttons
                        size_hint_y: None
                        size_hint_x: None
                        width: self.minimum_width
                        pos_hint: {'center_x': 0.5, 'center_y': 0.5}
                        spacing: "5dp"
                        padding: "5dp"
                        IDButton:
                            on_release: root.performAction((self))
                            buttonid: -1
                            text: "Cancel"
                            id: action_button
                            font_size: '12sp'
                            padding: '12sp'
                            md_bg_color: root.getColor('gray')
                            color: root.getColor('white')


<DateTimeBox>:
    canvas:
        Color:
            rgba: (0,0,0, .4)
        Rectangle:
            size: self.size
            pos: self.pos
    MDCard:
        radius: "10dp"
        elevation: '4dp'
        pos_hint: {'center_x': .5, 'center_y': .5}
        size_hint_x: .8
        size_hint_y: None
        md_bg_color: root.getColor('white')
        height: datetime_box_layout.height
        id: card
        StackLayout:
            id: datetime_box_layout
            size_hint_y: None
            height: self.minimum_height
            padding: '20dp'
            Label:
                text: "Do This Action?"
                id: title
                font_name: 'Roboto'
                font_size: '16sp'
                pos_hint: {'center_x': 0.5}
                size_hint_y: None
                color: root.getColor('primary')
                bold: True
            Label:
                text: "Here are some details about the action you are about to do!"
                id: contents
                font_name: 'Roboto'
                font_size: '12sp'
                pos_hint: {'center_x': 0.5}
                size_hint_y: None
                color: root.getColor('secondary')
                text_size: self.width, None
                height: self.texture_size[1]
                halign: 'center'
                markup: True
            StackLayout:
                size_hint_y: None
                height: self.minimum_height
                padding: ('20dp', '10dp', '20dp', '0dp')
                spacing: '4dp'
                Label:
                    text: "Date of Event"
                    font_name: 'Roboto'
                    font_size: '14sp'
                    pos_hint: {'center_x': 0.5}
                    size_hint_y: None
                    color: root.getColor('primary')
                    bold: True
                FloatLayout:
                    size_hint_y: None
                    pos_hint: {'center_x': 0.5, 'center_y': 0.5}
                    height: buttons.height
                    GridLayout:
                        rows: 1
                        id: buttons
                        size_hint_y: None
                        size_hint_x: None
                        width: self.minimum_width
                        pos_hint: {'center_x': 0.5, 'center_y': 0.5}
                        spacing: "8dp"
                        padding: "5dp"
                        MDTextField:
                            id: m_date
                            multiline: False
                            icon_right_color: root.getColor('primary')
                            line_color_focus: root.getColor('primary')
                            active_line: True
                            hint_text: 'MM'
                            size_hint_x: None
                            width: '40dp'
                            pos_hint: {'center_x': 0.5}
                            font_name: 'Roboto'
                            border: (2,2,2,2)
                            border_color: 0,0,0,1
                            on_text: root.dateChange("M")
                            input_filter: "int"
                        MDTextField:
                            id: d_date
                            multiline: False
                            icon_right_color: root.getColor('primary')
                            line_color_focus: root.getColor('primary')
                            active_line: True
                            hint_text: 'DD'
                            size_hint_x: None
                            width: '40dp'
                            pos_hint: {'center_x': 0.5}
                            font_name: 'Roboto'
                            border: (2,2,2,2)
                            border_color: 0,0,0,1
                            on_text: root.dateChange("D")
                            input_filter: "int"
                        MDTextField:
                            id: y_date
                            multiline: False
                            icon_right_color: root.getColor('primary')
                            line_color_focus: root.getColor('primary')
                            active_line: True
                            hint_text: 'YYYY'
                            size_hint_x: None
                            width: '60dp'
                            pos_hint: {'center_x': 0.5}
                            font_name: 'Roboto'
                            border: (2,2,2,2)
                            border_color: 0,0,0,1
                            on_text: root.dateChange("Y")
                            input_filter: "int"
                Label:
                    text: "Time of Event"
                    font_name: 'Roboto'
                    font_size: '14sp'
                    pos_hint: {'center_x': 0.5}
                    size_hint_y: None
                    color: root.getColor('primary')
                    bold: True
                FloatLayout:
                    size_hint_y: None
                    pos_hint: {'center_x': 0.5, 'center_y': 0.5}
                    height: buttons2.height
                    GridLayout:
                        rows: 1
                        id: buttons2
                        size_hint_y: None
                        size_hint_x: None
                        width: self.minimum_width
                        pos_hint: {'center_x': 0.5, 'center_y': 0.5}
                        spacing: "8dp"
                        padding: "5dp"
                        MDTextField:
                            id: h_time
                            multiline: False
                            icon_right_color: root.getColor('primary')
                            line_color_focus: root.getColor('primary')
                            active_line: True
                            hint_text: 'HH'
                            size_hint_x: None
                            width: '40dp'
                            pos_hint: {'center_x': 0.5}
                            font_name: 'Roboto'
                            border: (2,2,2,2)
                            border_color: 0,0,0,1
                            on_text: root.timeChange("H")
                            input_filter: "int"
                        MDTextField:
                            id: m_time
                            multiline: False
                            icon_right_color: root.getColor('primary')
                            line_color_focus: root.getColor('primary')
                            active_line: True
                            hint_text: 'MM'
                            size_hint_x: None
                            width: '40dp'
                            pos_hint: {'center_x': 0.5}
                            font_name: 'Roboto'
                            border: (2,2,2,2)
                            border_color: 0,0,0,1
                            on_text: root.timeChange("M")
                            input_filter: "int"
                        
                
                FloatLayout:
                    size_hint_y: None
                    pos_hint: {'center_x': 0.5, 'center_y': 0.5}
                    height: defaultbuttons.height
                    GridLayout:
                        rows: 1
                        id: defaultbuttons
                        size_hint_y: None
                        size_hint_x: None
                        width: self.minimum_width
                        pos_hint: {'center_x': 0.5, 'center_y': 0.5}
                        spacing: "5dp"
                        padding: "5dp"
                        IDButton:
                            on_release: root.performAction((self))
                            buttonid: -1
                            text: "Cancel"
                            id: action_button
                            font_size: '12sp'
                            padding: '12sp'
                            md_bg_color: root.getColor('gray')
                            color: root.getColor('white')
                        IDButton:
                            on_release: root.performAction((self))
                            buttonid: 0
                            text: "Submit"
                            id: submit_button
                            font_size: '12sp'
                            padding: '12sp'
                            md_bg_color: root.getColor('primary')
                            color: root.getColor('white')
                    


